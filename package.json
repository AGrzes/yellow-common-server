{
  "name": "yellow-common-server",
  "version": "1.0.0",
  "description": "",
  "main": "target",
  "scripts": {
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "test": "nyc mocha test/**/*.{ts,tsx}",
    "build": "rm -rf target && tsc",
    "watch": "tsc -w --incremental",
    "format": "tslint 'src/**/*.ts' --fix",
    "lint": "tslint 'src/**/*.ts'"
  },
  "nyc": {
    "all": true,
    "extension": [
      ".ts",
      ".tsx"
    ],
    "exclude": [
      "**/*.d.ts"
    ],
    "include": [
      "src/**/*"
    ],
    "reporter": [
      "html",
      "text",
      "lcov"
    ],
    "require": [
      "ts-node/register",
      "source-map-support/register"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AGrzes/yellow-common-server.git"
  },
  "author": "Grzegorz Abramczyk <abramczykg@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/AGrzes/yellow-common-server/issues"
  },
  "homepage": "https://github.com/AGrzes/yellow-common-server#readme",
  "dependencies": {
    "@types/chai": "^4.2.2",
    "@types/debug": "^4.1.5",
    "@types/express": "^4.17.1",
    "@types/lodash": "^4.14.138",
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.7.4",
    "axios": "^0.19.0",
    "chai": "^4.2.0",
    "coveralls": "^3.0.6",
    "debug": "^4.1.1",
    "express": "^4.17.1",
    "lodash": "^4.17.15",
    "mocha": "^6.2.0",
    "nyc": "^14.1.1",
    "pouchdb": "^7.1.1",
    "source-map-support": "^0.5.13",
    "ts-node": "^8.3.0",
    "tslint": "^5.20.0",
    "typescript": "^3.6.3"
  }
}
